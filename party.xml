<tryton>
    <data>
        <!-- relacionado-->
        <record model="ir.action.act_window" id="act_party_payable_form">
            <field name="name">Por Pagar</field>
            <field name="res_model">purchase.purchase</field>
            <field name="domain">[('party', 'in', Eval('active_ids')), ('state', '=', 'confirmed')]</field>
        </record>
        <record model="ir.action.keyword"
                id="act_open_party_payable_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">party.party,-1</field>
            <field name="action" ref="act_party_payable_form"/>
        </record>

    </data>
</tryton>
